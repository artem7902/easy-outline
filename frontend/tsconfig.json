{
  "extends": "./tsconfig.paths.json",
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react",
    "types": ["node"],
    "baseUrl": "./src",
    "paths": {
      "@components/*": [
        "components/*"
      ],
      "@models/*": [
        "models/*"
      ],
      "@redux/*": [
        "redux/*"
      ],
      "@api/*": [
        "api/*"
      ],
      "@hooks": [
        "hooks"
      ],
      "@config/*": [
        "config/*"
      ],
      "@utils": [
        "utils"
      ],
    }
  },
  "include": [
    "src"
  ]
}
